{
    "name": "@tokens-studio/graph-engine",
    "version": "0.0.1",
    "description": "An execution engine to handle Token Studios generators and resolvers",
    "author": "andrew@hyma.io",
    "license": "MIT",
    "type": "module",
    "exports": {
        "./nodes/": {
            "import": "./dist/esm/nodes/",
            "require": "./dist/cjs/nodes/",
            "types": "./dist/esm/nodes/"
        },
        ".": {
            "import": "./dist/esm/index.js",
            "require": "./dist/cjs/index.js",
            "types": "./dist/esm/index.d.ts"
        }
    },
    "main": "./dist/cjs/index.js",
    "module": "./dist/esm.js",
    "files": [
        "cli",
        "dist"
    ],
    "scripts": {
        "docs": "typedoc",
        "build": "rollup --bundleConfigAsCjs --config rollup.config.mjs",
        "dev": "rollup -w --bundleConfigAsCjs --config rollup.config.mjs",
        "format": "npm run format:eslint && npm run format:prettier",
        "format:eslint": "eslint --ext .tsx,.ts,.js,.html . --fix",
        "format:prettier": "prettier \"**/*.{ts,js,md,json}\" --write",
        "lint": "eslint .",
        "lint:fix": "eslint --fix .",
        "test": "jest",
        "release": "npm run build && changeset publish"
    },
    "sideEffects": false,
    "types": "./dist/esm/index.d.ts",
    "dependencies": {
        "@tokens-studio/types": "^0.2.3",
        "apca-w3": "^0.1.9",
        "chroma-js": "^2.4.2",
        "colorjs.io": "^0.4.3",
        "dot-prop": "^8.0.0",
        "graphlib": "^2.1.8",
        "is-promise": "^4.0.0",
        "postcss-value-parser": "^4.2.0",
        "reactflow": "^11.7.2",
        "token-transformer": "^0.0.32"
    },
    "devDependencies": {
        "@babel/preset-env": "^7.21.5",
        "@changesets/cli": "2.26.0",
        "@jest/globals": "^29.5.0",
        "@types/jest": "28.1.0",
        "@types/node": "^18.15.11",
        "@types/react": "^18.2.6",
        "@typescript-eslint/eslint-plugin": "5.59.1",
        "eslint": "8.30.0",
        "eslint-config-custom": "*",
        "eslint-plugin-jest": "^27.2.1",
        "eslint-plugin-sort-imports-es6-autofix": "0.6.0",
        "jest": "28.1.0",
        "jest-junit": "^15.0.0",
        "lint-staged": "^13.2.1",
        "react": "^18.2.0",
        "rollup": "^3.10.1",
        "rollup-plugin-cleanup-dir": "^1.0.8",
        "rollup-plugin-typescript2": "^0.34.1",
        "ts-jest": "28.0.3",
        "ts-jest-resolver": "^2.0.1",
        "ts-node": "^10.9.1",
        "typedoc": "^0.24.7",
        "typescript": "^5.0.4",
        "typescript-transform-paths": "^3.4.6"
    },
    "keywords": [
        "ui",
        "studio",
        "tokens"
    ],
    "jsnext:main": "./dist/esm/index.js"
}