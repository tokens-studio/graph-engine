{
  "name": "@tokens-studio/graph-engine-nodes-audio",
  "version": "1.0.0",
  "description": "Audio nodes for the graph engine",
  "license": "MIT",
  "author": "andrew@hyma.io",
  "type": "module",
  "exports": {
    "./*": {
      "import": "./dist/*",
      "require": "./dist/*",
      "types": "./dist/*"
    },
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "docs": "typedoc",
    "format": "npm run format:eslint && npm run format:prettier",
    "format:eslint": "eslint . --fix",
    "format:prettier": "prettier \"**/*.{tsx,ts,js,md,json}\" --write",
    "lint": "npm run lint:prettier && npm run lint:eslint",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier \"**/*.{tsx,ts,js,md,json}\" --check",
    "release": "npm run build && changeset publish",
    "test": "jest --passWithNoTests"
  },
  "dependencies": {
    "audio-buffer-from": "^1.1.1"
  },
  "peerDependencies": {
    "@tokens-studio/graph-engine": "*",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.21.5",
    "@changesets/cli": "^2.26.0",
    "@jest/globals": "^29.5.0",
    "@tokens-studio/eslint-config-custom": "*",
    "@tokens-studio/prettier-config-custom": "*",
    "@tokens-studio/graph-engine": "*",
    "@types/jest": "^28.1.0",
    "@types/node": "^18.15.11",
    "jest": "^28.1.0",
    "jest-junit": "^15.0.0",
    "ts-jest": "^28.0.3",
    "ts-jest-resolver": "^2.0.1",
    "ts-node": "^10.9.1",
    "tsup": "^8.1.0",
    "typedoc": "^0.24.7",
    "typescript": "^5.4.5"
  },
  "keywords": [
    "studio",
    "tokens",
    "ui"
  ],
  "jsnext:main": "./dist/index.js"
}