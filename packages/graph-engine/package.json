{
  "name": "@tokens-studio/graph-engine",
  "version": "0.17.4",
  "description": "An execution engine to handle Token Studios generators and resolvers",
  "license": "MIT",
  "author": "andrew@hyma.io",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "alias": "tsc-alias -p tsconfig.prod.json --dir dist/esm",
    "build": "tsup",
    "dev": "tsup --watch",
    "docs": "typedoc",
    "format": "npm run format:eslint && npm run format:prettier",
    "format:eslint": "eslint --ext .tsx,.ts,.js,.html . --fix",
    "format:prettier": "prettier \"**/*.{ts,js,md,json}\" --write",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "release": "npm run build && changeset publish",
    "test": "jest"
  },
  "sideEffects": false,
  "types": "./dist/index.d.ts",
  "dependencies": {
    "@tokens-studio/types": "^0.3.0",
    "ajv": "^8.12.0",
    "chroma-js": "^2.4.2",
    "color-blind": "^0.1.3",
    "color-namer": "^1.4.0",
    "colorjs.io": "^0.5.0",
    "culori": "^3.1.2",
    "deepmerge": "^4.3.1",
    "dot-prop": "^8.0.2",
    "expr-eval": "^2.0.2",
    "json-schema-defaults": "^0.4.0",
    "lodash.orderby": "^4.6.0",
    "mdn-data": "^2.3.0",
    "mobx": "^6.12.0",
    "poline": "^0.7.0",
    "postcss-value-parser": "^4.2.0",
    "semver-compare": "^1.0.0",
    "token-transformer": "^0.0.33",
    "uuid": "^9.0.1"
  },
  "peerDependencies": {
    "@tokens-studio/types": "^0.3.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.21.5",
    "@changesets/cli": "2.26.0",
    "@jest/globals": "^29.5.0",
    "eslint-config-custom": "*",
    "@types/chroma-js": "^2.4.0",
    "@types/culori": "^2.0.0",
    "@types/jest": "28.1.0",
    "@types/lodash.orderby": "^4.6.7",
    "@types/node": "^18.15.11",
    "jest": "28.1.0",
    "jest-junit": "^15.0.0",
    "ts-jest": "28.0.3",
    "ts-jest-resolver": "^2.0.1",
    "ts-node": "^10.9.1",
    "tsup": "^8.0.2",
    "typedoc": "^0.24.7",
    "typescript": "4.8.4",
    "typescript-transform-paths": "^3.4.6"
  },
  "keywords": [
    "studio",
    "tokens",
    "ui"
  ],
  "jsnext:main": "./dist/index.js"
}