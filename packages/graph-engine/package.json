{
  "name": "@tokens-studio/graph-engine",
  "version": "1.0.0",
  "description": "An execution engine to handle Token Studios generators and resolvers",
  "license": "MIT",
  "author": "andrew@hyma.io",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "alias": "tsc-alias -p tsconfig.prod.json --dir dist/esm",
    "build": "tsup",
    "dev": "tsup --watch",
    "docs": "typedoc && typedoc --out docs --json ./docs/docs.json",
    "format": "npm run format:eslint && npm run format:prettier",
    "format:eslint": "eslint . --fix",
    "format:prettier": "prettier \"**/*.{tsx,ts,js,md,json}\" --write",
    "lint": "npm run lint:prettier && npm run lint:eslint",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier \"**/*.{tsx,ts,js,md,json}\" --check",
    "release": "npm run build && changeset publish",
    "test": "web-test-runner --coverage"
  },
  "dependencies": {
    "@bundled-es-modules/deepmerge": "^4.3.1",
    "@tokens-studio/types": "^0.3.0",
    "z-schema-esm": "^6.0.1",
    "color-blind-esm": "^0.1.4",
    "color-namer": "^1.4.0",
    "colorjs.io": "^0.5.0",
    "culori": "^3.1.2",
    "dot-prop": "^8.0.2",
    "expr-eval": "^2.0.2",
    "json-schema-defaults-esm": "^1.0.4",
    "lodash-es": "^4.17.21",
    "mdn-data": "^2.3.0",
    "mobx": "^6.12.0",
    "poline": "^0.7.0",
    "postcss-value-parser-esm": "^4.2.0",
    "compare-versions": "^6.1.0",
    "token-transformer": "^0.0.33",
    "uuid": "^9.0.1"
  },
  "peerDependencies": {
    "@tokens-studio/types": "^0.3.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.21.5",
    "@changesets/cli": "^2.26.0",
    "@tokens-studio/eslint-config-custom": "*",
    "@tokens-studio/prettier-config-custom": "*",
    "@types/culori": "^2.0.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^18.15.11",
    "@web/test-runner": "^0.18.2",
    "@web/dev-server-esbuild": "^1.0.2",
    "@web/test-runner-playwright": "^0.11.0",
    "chai": "^5.1.1",
    "tsup": "^8.1.0",
    "typedoc": "^0.24.7",
    "typescript": "^5.4.5",
    "typescript-transform-paths": "^3.4.6"
  },
  "keywords": [
    "studio",
    "tokens",
    "ui"
  ]
}