{
  "name": "@tokens-studio/graph-engine",
  "version": "0.10.0",
  "description": "An execution engine to handle Token Studios generators and resolvers",
  "license": "MIT",
  "author": "andrew@hyma.io",
  "type": "module",
  "exports": {
    "./nodes/": {
      "import": "./dist/esm/nodes/",
      "require": "./dist/cjs/nodes/",
      "types": "./dist/esm/nodes/"
    },
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/esm/index.d.ts"
    }
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm.js",
  "files": [
    "cli",
    "dist"
  ],
  "scripts": {
    "build": "rollup --bundleConfigAsCjs --config rollup.config.mjs",
    "dev": "rollup -w --bundleConfigAsCjs --config rollup.config.mjs",
    "docs": "typedoc",
    "format": "npm run format:eslint && npm run format:prettier",
    "format:eslint": "eslint --ext .tsx,.ts,.js,.html . --fix",
    "format:prettier": "prettier \"**/*.{ts,js,md,json}\" --write",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "release": "npm run build && changeset publish",
    "test": "jest"
  },
  "sideEffects": false,
  "types": "./dist/esm/index.d.ts",
  "dependencies": {
    "@tokens-studio/types": "^0.2.3",
    "ajv": "^8.12.0",
    "apca-w3": "^0.1.9",
    "chroma-js": "^2.4.2",
    "color-blind": "^0.1.3",
    "colorjs.io": "^0.4.3",
    "culori": "^3.1.2",
    "dot-prop": "^8.0.0",
    "extract-colors": "^3.0.0",
    "get-pixels": "^3.3.3",
    "is-promise": "^4.0.0",
    "lodash.orderby": "^4.6.0",
    "poline": "^0.7.0",
    "postcss-value-parser": "^4.2.0",
    "react-mosaic-component": "^6.0.1",
    "reactflow": "11.7.0",
    "token-transformer": "^0.0.32"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.21.5",
    "@changesets/cli": "2.26.0",
    "@jest/globals": "^29.5.0",
    "@swc/core": "^1.3.60",
    "@tokens-studio/eslint-config-custom": "*",
    "@types/chroma-js": "^2.4.0",
    "@types/culori": "^2.0.0",
    "@types/jest": "28.1.0",
    "@types/lodash.orderby": "^4.6.7",
    "@types/node": "^18.15.11",
    "@types/react": "^18.2.6",
    "husky": "^8.0.3",
    "jest": "28.1.0",
    "jest-junit": "^15.0.0",
    "lint-staged": "^13.2.2",
    "react": "^18.2.0",
    "rollup": "^3.10.1",
    "rollup-plugin-cleanup-dir": "^1.0.8",
    "rollup-plugin-swc": "^0.2.1",
    "rollup-plugin-typescript2": "^0.34.1",
    "ts-jest": "28.0.3",
    "ts-jest-resolver": "^2.0.1",
    "ts-node": "^10.9.1",
    "typedoc": "^0.24.7",
    "typescript": "^5.0.4",
    "typescript-transform-paths": "^3.4.6"
  },
  "keywords": [
    "studio",
    "tokens",
    "ui"
  ],
  "jsnext:main": "./dist/esm/index.js"
}
