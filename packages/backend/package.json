{
  "name": "@tokens-studio/graph-engine-backed",
  "version": "0.0.1",
  "description": "Execution graph backend for apis",
  "license": "MIT",
  "author": "andrew@hyma.io",
  "type": "module",
  "exports": {
    "./nodes/*": {
      "import": "./dist/esm/nodes/*",
      "require": "./dist/cjs/nodes/*",
      "types": "./dist/esm/nodes/*"
    },
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/esm/index.d.ts"
    }
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "routes": "tsoa spec-and-routes",
    "alias": "tsc-alias -p tsconfig.prod.json --dir dist/esm",
    "build": "tsoa spec-and-routes && tsc",
    "dev": "nodemon -w src --ext ts --exec node --loader ./scripts/loader.js src/index.ts",
    "docs": "typedoc",
    "format": "npm run format:eslint && npm run format:prettier",
    "format:eslint": "eslint --ext .tsx,.ts,.js,.html . --fix",
    "format:prettier": "prettier \"**/*.{ts,js,md,json}\" --write",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "release": "npm run build && changeset publish",
    "test": "jest"
  },
  "sideEffects": false,
  "types": "./dist/esm/index.d.ts",
  "dependencies": {
    "body-parser": "^1.20.2",
    "express": "^4.19.2",
    "express-prometheus-middleware": "^1.2.0",
    "swagger-ui-express": "^5.0.0",
    "tsoa": "^6.2.0",
    "typedi": "^0.10.0"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@opentelemetry/api": "^1.8.0",
    "@opentelemetry/auto-instrumentations-node": "^0.44.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.7",
    "supertest": "^6.3.4",
    "typescript": "^5.4.5"
  },
  "keywords": [
    "studio",
    "tokens",
    "graph",
    "backend"
  ],
  "jsnext:main": "./dist/esm/index.js"
}
