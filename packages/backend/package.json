{
  "name": "@tokens-studio/graph-engine-backend",
  "version": "0.0.1",
  "description": "Execution graph backend for apis",
  "license": "MIT with CC",
  "author": "andrew@hyma.io",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "routes": "tsoa spec-and-routes",
    "build": "npm run routes && tsup",
    "live": "concurrently \"nodemon -x tsoa spec-and-routes\" \"nodemon\"",
    "dev": "npm run routes && nodemon",
    "docs": "typedoc",
    "start": "node ./dist/index.js",
    "format": "npm run format:eslint && npm run format:prettier",
    "format:eslint": "eslint . --fix",
    "format:prettier": "prettier \"**/*.{tsx,ts,js,md,json}\" --write",
    "lint": "npm run lint:prettier && npm run lint:eslint",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier \"**/*.{tsx,ts,js,md,json}\" --check",
    "release": "npm run build && changeset publish",
    "test": "jest --passWithNoTests"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.10.6",
    "@opentelemetry/api": "^1.8.0",
    "@opentelemetry/auto-instrumentations-node": "^0.44.0",
    "@ory/keto-grpc-client": "0.11.1-alpha.0",
    "@prisma/client": "^5.13.0",
    "body-parser": "^1.20.2",
    "concurrently": "^8.2.2",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "http-status-codes": "^2.3.0",
    "inversify": "^6.0.2",
    "inversify-binding-decorators": "^4.0.0",
    "reflect-metadata": "^0.2.2",
    "swagger-ui-express": "^5.0.0",
    "tsoa": "^6.2.0",
    "winston": "^3.13.0"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@opentelemetry/api": "^1.8.0",
    "@opentelemetry/auto-instrumentations-node": "^0.44.0",
    "@ory/kratos-client": "^1.1.0",
    "@tokens-studio/graph-engine": "*",
    "@tokens-studio/eslint-config-custom": "*",
    "@tokens-studio/prettier-config-custom": "*",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "@types/swagger-ui-express": "^4.1.6",
    "nodemon": "^3.1.0",
    "prisma": "^5.13.0",
    "supertest": "^6.3.4",
    "tsup": "^8.1.0",
    "tsx": "^4.7.2",
    "typescript": "^5.4.5"
  },
  "keywords": [
    "studio",
    "tokens",
    "graph",
    "backend"
  ]
}