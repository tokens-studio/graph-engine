{
  "name": "@tokens-studio/graph-engine-backend",
  "version": "0.0.1",
  "description": "Execution graph backend for apis",
  "license": "MIT with CC",
  "author": "andrew@hyma.io",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/esm/index.d.ts"
    }
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "routes": "tsoa spec-and-routes",
    "build": "tsup",
    "live": "nodemon",
    "dev": "concurrently \"nodemon\" \"nodemon -x tsoa spec-and-routes\"",
    "docs": "typedoc",
    "start": "node ./dist/index.js",
    "format": "npm run format:eslint && npm run format:prettier",
    "format:eslint": "eslint --ext .tsx,.ts,.js,.html . --fix",
    "format:prettier": "prettier \"**/*.{ts,js,md,json}\" --write",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "release": "npm run build && changeset publish",
    "test": "jest"
  },
  "sideEffects": true,
  "types": "./dist/esm/index.d.ts",
  "dependencies": {
    "@grpc/grpc-js": "^1.10.6",
    "@opentelemetry/api": "^1.8.0",
    "@opentelemetry/auto-instrumentations-node": "^0.44.0",
    "@ory/keto-grpc-client": "0.11.1-alpha.0",
    "@prisma/client": "^5.13.0",
    "body-parser": "^1.20.2",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-prometheus-middleware": "^1.2.0",
    "http-status-codes": "^2.3.0",
    "inversify": "^6.0.2",
    "inversify-binding-decorators": "^4.0.0",
    "prom-client": "^15.1.2",
    "reflect-metadata": "^0.2.2",
    "swagger-ui-express": "^5.0.0",
    "tsoa": "^6.2.0",
    "winston": "^3.13.0"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@opentelemetry/api": "^1.8.0",
    "@opentelemetry/auto-instrumentations-node": "^0.44.0",
    "@ory/kratos-client": "^1.1.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "@types/swagger-ui-express": "^4.1.6",
    "nodemon": "^3.1.0",
    "prisma": "^5.13.0",
    "supertest": "^6.3.4",
    "tsup": "^8.0.2",
    "tsx": "^4.7.2",
    "typescript": "^5.4.5"
  },
  "keywords": [
    "studio",
    "tokens",
    "graph",
    "backend"
  ],
  "jsnext:main": "./dist/esm/index.js"
}
