{
  "private": true,
  "name": "@tokens-studio/graph-engine-ui",
  "version": "3.2.1",
  "description": "UI for the graph engine",
  "author": "andrew@hyma.io",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/tokens-studio/graph-engine"
  },
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=8192 next build",
    "profile": "node --max-old-space-size=8192 --heap-prof ../../node_modules/next/dist/bin/next build -d --experimental-debug-memory-usage",
    "dev": "next",
    "format": "npm run format:eslint && npm run format:prettier",
    "format:eslint": "eslint . --fix",
    "format:prettier": "prettier \"**/*.{tsx,ts,js,md,json}\" --write",
    "lint": "npm run lint:prettier && npm run lint:eslint",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier \"**/*.{tsx,ts,js,md,json}\" --check",
    "start": "next start",
    "lint:types": "tsc --noEmit",
    "db:types": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy"
  },
