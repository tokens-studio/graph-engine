{
  "name": "@tokens-studio/graph-engine-migration",
  "version": "0.12.0",
  "description": "A package to upgrade old graph engine files to the new format",
  "license": "MIT",
  "author": "andrew@hyma.io",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/esm/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "format": "npm run format:eslint && npm run format:prettier",
    "format:eslint": "eslint . --fix",
    "format:prettier": "prettier \"**/*.{tsx,ts,js,md,json}\" --write",
    "lint": "npm run lint:prettier && npm run lint:eslint",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier \"**/*.{tsx,ts,js,md,json}\" --check",
    "release": "npm run build && changeset publish",
    "test": "jest --passWithNoTests"
  },
  "sideEffects": false,
  "dependencies": {
    "@tokens-studio/graph-engine": "*",
    "semver-compare": "^1.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.21.5",
    "@changesets/cli": "2.26.0",
    "@jest/globals": "^29.5.0",
    "@tokens-studio/eslint-config-custom": "*",
    "@tokens-studio/prettier-config-custom": "*",
    "@types/jest": "28.1.0",
    "@types/node": "^18.15.11",
    "jest": "28.1.0",
    "jest-junit": "^15.0.0",
    "lint-staged": "^13.2.2",
    "react": "^18.2.0",
    "ts-jest": "28.0.3",
    "ts-jest-resolver": "^2.0.1",
    "ts-node": "^10.9.1",
    "tsup": "^8.1.0",
    "typescript": "^5.4.5",
    "typescript-transform-paths": "^3.4.6"
  },
  "keywords": [
    "studio",
    "tokens",
    "ui"
  ],
  "jsnext:main": "./dist/index.cjs"
}