{
  "private": false,
  "name": "@tokens-studio/graph-editor",
  "version": "1.3.0",
  "description": "UI for the graph engine",
  "author": "pop@hyma.io",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs"
    },
    "./index.css": "./dist/index.css"
  },
  "module": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup --external reactflow,@reactflow/minimap",
    "dev": "tsup --watch --external reactflow",
    "format": "npm run format:eslint && npm run format:prettier",
    "format:eslint": "eslint --ext .tsx,.ts,.js,.html . --fix",
    "format:prettier": "prettier \"**/*.{ts,tsx,js,md,json}\" --write",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "release": "npm run build && changeset publish",
    "test": "jest --passWithNoTests"
  },
  "types": "dist/index.d.ts",
  "dependencies": {
    "@iconicicons/react": "1.5.1",
    "@radix-ui/react-accordion": "1.1.1",
    "@radix-ui/react-context-menu": "2.1.3",
    "@radix-ui/react-dialog": "1.0.4",
    "@radix-ui/react-icons": "1.3.0",
    "@radix-ui/react-slider": "1.1.1",
    "@reactflow/node-resizer": "2.1.0",
    "@rematch/core": "2.2.0",
    "@rematch/persist": "2.1.2",
    "@stitches/react": "1.2.8",
    "@tokens-studio/graph-engine": "*",
    "@tokens-studio/tokens": "0.0.21",
    "@tokens-studio/types": "0.2.3",
    "@tokens-studio/ui": "0.3.0",
    "change-case": "4.1.2",
    "classnames": "2.3.2",
    "copy-to-clipboard": "3.3.3",
    "dagre": "0.8.5",
    "debounce": "1.2.1",
    "dot-prop": "8.0.0",
    "focus-trap-react": "10.1.1",
    "graphlib": "2.1.8",
    "is-promise": "4.0.0",
    "json5": "2.2.3",
    "jszip": "3.10.1",
    "poline": "^0.7.0",
    "postcss-value-parser": "4.2.0",
    "react-color": "2.19.3",
    "react-contexify": "^6.0.0",
    "react-drag-reorder": "1.2.0",
    "react-error-boundary": "4.0.4",
    "react-folder-tree": "5.1.1",
    "react-hotkeys": "2.0.0",
    "react-redux": "8.0.5",
    "react-resizable-layout": "0.6.0",
    "react-use": "17.4.0",
    "reactflow": "^11.8.3",
    "redux": "4.2.1",
    "redux-persist": "6.0.0",
    "reselect": "4.1.8",
    "sentence-case": "3.0.4",
    "uuid": "9.0.0"
  },
  "peerDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/graphlib": "2.1.8",
    "@types/react": "18.0.37",
    "@types/react-dom": "18.0.11",
    "prettier": "2.8.7",
    "tsup": "^7.2.0",
    "typescript": "5.0.4"
  },
  "jsnext:main": "./dist/index.js"
}
